  "scripts": {
    "start": "REACT_APP_AUTH_DOMAIN=${AUTH_DOMAIN} REACT_APP_AUTH_CLIENT_ID=${AUTH_CLIENT_ID} REACT_APP_AUTH_AUDIENCE=${AUTH_AUDIENCE} REACT_APP_API_URL=http://${API_URL} react-scripts start",
    "build": "react-scripts build",
    "build:dev": "REACT_APP_AUTH_DOMAIN=${AUTH_DOMAIN} REACT_APP_AUTH_CLIENT_ID=${AUTH_CLIENT_ID} REACT_APP_AUTH_AUDIENCE=${AUTH_AUDIENCE} REACT_APP_API_URL=api.dev.brightestbio.com npm run build --production",
